{{#defineZone "sidebarMenu"}}
    <ul class="sidebar-nav">
        <li class="sidebar-brand">
            <a href="#">
                <i class="fw {{defineZone "sidebarMenu-icon"}}"></i>
                {{defineZone "sidebarMenu-name"}}
            </a>
        </li>
        {{#defineZone "sidebarMenu-item"}}
            <li>
                <a href="{{defineZone "sidebarMenu-item-uri"}}">
                    <i class="fw {{defineZone "sidebarMenu-item-icon"}}"></i>
                    {{defineZone "sidebarMenu-item-name"}}
                </a>
            </li>
        {{/defineZone}}
    </ul>
{{/defineZone}}