{
    "displayName": "UUF Template App",
    "cachingEnabled": true,
    "debuggingEnabled": false,
    "logLevel": "debug",
    "welcomeFile": "",
    "errorPages": [
        {
            "code": "500",
            "path": "/error/server.hbs"
        },
        {
            "code": "404",
            "path": "/client/not-found.hbs"
        }
    ],
    "securityConstraints": [
        {
            "urlPattern": "/asset/{operation}",
            "permissions": [
                "publisher/asset/create",
                "publisher/asset/edit"
            ]
        },
        {
            "urlPattern": "/dashboard/{assetType}/view/{deviceType}",
            "permissions": [
                "store/mobile/view",
                "store/web/view"
            ]
        }
    ]
}