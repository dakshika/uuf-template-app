<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{{#defineZone "title"}}WSO2 Template{{/defineZone}}</title>
    {{defineZone "favicon"}}
    {{defineZone "themeCss"}}
    {{defineZone "topCss"}}
    {{defineZone "topJs"}}
</head>
<body>

<!-- header -->
{{defineZone "header"}}
<!-- /.header -->

<!-- navbar -->
<div class="navbar-wrapper">
    {{defineZone "navbars"}}
</div>
{{defineZone "navbarsBelow"}}
<!-- /.navbar -->

<!-- .left-sidebars -->
{{defineZone "sidePanes"}}
<!-- /.left-sidebars -->

<!-- .right-sidebar -->
{{defineZone "rightSidebars"}}
<!-- /.right-sidebars -->

<!-- #page-content-wrapper -->
<div class="page-content-wrapper">
    <!-- page content -->
    <div class="container-fluid body-wrapper">
        {{defineZone "content"}}
    </div>
</div>
<!-- /#page-content-wrapper -->

<!-- footer -->
<footer class="footer">
    <div class="container-fluid">
        {{#defineZone "footer"}}
            <p>
                WSO2 | &copy; 2015
                <a href="http://wso2.com/" target="_blank"><i class="icon fw fw-wso2"></i> Inc</a>.
            </p>
        {{/defineZone}}
    </div>
</footer>

{{defineZone "bottomJs"}}

</body>
</html>